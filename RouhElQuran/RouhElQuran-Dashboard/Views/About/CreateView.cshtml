@model Core.HelperModel.FileModel.FileUpload

<form id="CreateFileFormId" asp-action="Create" asp-controller="About" method="post" enctype="multipart/form-data">
    <div class="row mb-4">
        <div class="col-md-6 form-control-validation fv-plugins-icon-container">
            <label class="form-label d-block text-start" for="fileId">Choose Your File</label>
            <input asp-for="FormFile" type="file" class="form-control" id="fileId" placeholder="Choose File" multiple />
            <span asp-validation-for="FormFile" class="text-danger"></span>
        </div>
        <div class="col-md-6 form-control-validation fv-plugins-icon-container">
            <label class="form-label d-block text-start" for="NoteId">File Note</label>
            <input asp-for="Note" type="text" id="NoteId" class="form-control" placeholder="Write Note" />
            <span asp-validation-for="Note" class="text-danger"></span>
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

